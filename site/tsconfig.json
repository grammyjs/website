{
  "include": [
    "env.d.ts",
    "docs/.vitepress/**/*"
  ],
  "exclude": [
    "docs/.vitepress/dist/**/*",
    "docs/.vitepress/cache/**/*",
    "docs/.vitepress/.temp/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "module": "NodeNext",
    "resolveJsonModule": true,
    "types": [
      "node"
    ],
    "paths": {
      "node_modules/*": [
        "node_modules/*"
      ]
    }
  }
}
